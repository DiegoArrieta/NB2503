<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Calculo</servlet-name>
        <servlet-class>NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Trabajador</servlet-name>
        <servlet-class>Trabajador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ejercicio3</servlet-name>
        <servlet-class>Ejercicio3.Ejercicio3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Eje4</servlet-name>
        <servlet-class>Eje4.Eje4</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Calculo</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Trabajador</servlet-name>
        <url-pattern>/Trabajador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ejercicio3</servlet-name>
        <url-pattern>/Ejercicio3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Eje4</servlet-name>
        <url-pattern>/Eje4</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Eje4/Eje4.html</welcome-file>
    </welcome-file-list>
</web-app>
